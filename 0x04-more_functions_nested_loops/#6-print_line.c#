#include "holberton.h"

/**
 * print_line - Function that draws a straight line in the terminal.
 * @n: int.
 * Return: Always 0.
 */
void print_line(int n)
{

  int m;
  if (n > 0)
    {
      for (m = 0; m < n; m++)
	{
	  _putchar('_');
	}
      else 
	_putchar('\n');
    }
  else                             
}