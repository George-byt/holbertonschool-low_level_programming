#include "holberton.h"
#include <stdio.h>

/**
 * _memcpy - Function that copies memory area.
 * @dest: pointer.
 * @src: pointer.
 * @n: integer.
 * Return: Nothing.
 */
char *_memcpy(char *dest, char *src, unsigned int n)
{
int i;
char *t = (char*)dest;
char *s = (char*)src;
for (i = 0; i < n; i++)
{
t[i] = s[i];
}
}
